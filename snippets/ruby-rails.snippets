# Rails

snippet logi
	logger.info  { "${1:message}" }${2}
snippet logd
	logger.debug { "${1:message}" }${2}
snippet logw
	logger.warn  { "${1:message}" }${2}
snippet loge
	logger.error { "${1:message}" }${2}
snippet logf
	logger.fatal { "${1:message}" }${2}
snippet stylesheet
	<%= stylesheet_link_tag '${1:css}', :media => '{$2}' %>
